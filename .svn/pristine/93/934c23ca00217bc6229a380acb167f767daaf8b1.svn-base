<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.ShowAdminRightActivity"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <com.hjq.bar.TitleBar
        android:id="@+id/app_titlebar"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="#ffffff"
        app:leftIcon="@drawable/back"
        app:title="设备管理器权限"
        app:titleColor="@color/colorAccent"
        app:titleSize="20dp"/>

    <fragment
        android:id="@+id/admin_right_bottom"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.grampus.hualauncherkai.fragment.AcquireDeviceAdmin"/>

</LinearLayout>